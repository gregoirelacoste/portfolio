generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  firstname String?
}

model Dossier {
  id          Int     @id @default(autoincrement())
  name        String
  description String?
  numero      String
}
