generator client {
  provider = "prisma-client-js"
  output   = "./clientGenerated"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  dossier Dossier? @relation(fields:[dossierId], references:[id])
  dossierId Int?
}

model Dossier {
  id          Int     @id @default(autoincrement())
  name        String
  description String?
  numero      String
  user        User[]
}
